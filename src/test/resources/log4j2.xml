<Configuration packages="de.seitenbau.ozghub.prozesspipeline" status="WARN"
    name="ozghub-prozess-pipeline">
  <Appenders>
    <Console name="stdout" target="SYSTEM_OUT">
      <PatternLayout charset="UTF-8" pattern="%d{HH:mm:ss} %p %m%n" />
    </Console>
    <ListAppender name="ListAppender" />
  </Appenders>
  <Loggers>
    <Logger level="INFO" name="ListAppender" />
    <Root level="INFO">
      <AppenderRef ref="ListAppender" />
    </Root>
  </Loggers>
</Configuration>
